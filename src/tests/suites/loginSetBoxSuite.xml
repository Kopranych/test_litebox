<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<!-- enabled="false" -->
<!-- include -->
<suite verbose="1" name="Example suite">

    <test name="Авторизация" preserve-order="true" enabled="true">
        <classes>
            <class name="tests.init.Login">
                <methods>

                    <include name="incorrectPassword"/>
                    <include name="incorrectLogin"/>
                    <include name="incorrectLoginAndPassword"/>
                    <include name="correctLoginUpper"/>
                    <include name="correctLoginUpperLower"/>
                    <include name="correctPasswordUpper"/>
                    <include name="correctPasswordUpperLower"/>
                    <include name="incorrectPasswordSpaceBegin"/>
                    <include name="incorrectPasswordSpaceEnd"/>
                    <include name="incorrectLoginSpaceBegin"/>
                    <include name="incorrectLoginSpaceEnd"/>
                    <include name="Login"/>

                </methods>
            </class>
        </classes>
    </test>

</suite>
